{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build (Debug)",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/discord-cli.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "Build (Release)",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-r",
                "win10-x64",
                "${workspaceFolder}/discord-cli.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        }
    ]
}