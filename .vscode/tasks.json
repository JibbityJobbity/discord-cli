{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/discord-cli.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build (Release)",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-c",
                "Release",
                "${workspaceFolder}/discord-cli.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}